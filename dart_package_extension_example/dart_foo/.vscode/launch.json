    "version": "0.2.0",
    "configurations": [
        {
            "name": "dart_foo",
            "cwd": "packages/dart_foo",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "dart_foo_devtools_extension + simulated environment",
            "cwd": "packages/dart_foo_devtools_extension",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=use_simulated_environment=true"
            ]
        },
        {
            "name": "dart_foo_devtools_extension (profile mode)",
            "cwd": "packages/dart_foo_devtools_extension",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "dart_foo_devtools_extension (release mode)",
            "cwd": "packages/dart_foo_devtools_extension",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}